[giggle]
	compact-mode = false
	main-window-geometry = 0x0+0+0
	main-window-maximized = true
	history-view-vpane-position = 369
	main-window-view = HistoryView
	file-view-vpane-position = 161
[user]
	name =
	email =
	signingkey = 1F52F68B!
[alias]
	co = checkout
	st = status
	pff = pull --ff-only
	pnff = pull --no-ff
	mff = merge --ff-only
	mnff = merge --no-ff
	los = log --simplify-merges
	lops = log -p -C --simplify-merges
	gr = log --graph --decorate
	grs = log --graph --decorate --simplify-merges
	sui = submodule update --init
	diffc = diff --cached
	setup = "!f() { git init && git add -f . && GIT_COMMITTER_DATE=451521442 GIT_AUTHOR_DATE=451521442 git commit -m 'initial import' && git tag start; };f"
	lsf = ls-files
	lsg = "!f() { git ls-files | grep \"$@\"; };f"
[i18n]
	commitencoding = UTF-8
[repack]
	UseDeltaBaseOffset = true
[transfer]
	unpackLimit = 5
[url "https://bmc@git.crustytoothpaste.net/git/bmc/"]
	insteadOf = dct:
[url "bmc@git.crustytoothpaste.net:/srv/scm/git/bmc/"]
	insteadOf = dcts:
[url "git@github.com:bk2204/"]
	insteadOf = gh:
[url "https://github.com/"]
	insteadOf = ghp:
[push]
	default = matching
[pull]
	rebase = false
[color]
	ui = auto
[sendemail]
	smtpuser = bmc
	smtppass =
	smtpencryption = tls
	smtpserver = castro.crustytoothpaste.net
	smtpserverport = 587
	suppresscc = author
	suppresscc = self
	suppresscc = sob
	assume8bitEncoding = UTF-8
	confirm = always
	composeencoding = UTF-8
	chainreplyto = false
[diff]
	renames = copies
	algorithm = histogram
[merge]
	conflictstyle = diff3
[core]
	precomposeunicode = true
	logAllRefUpdates = true
# vim: set noet:
